  <script src='https://files.cnblogs.com/zengxiangzhan/nprogress.js?v=2018'></script>
 <script>
NProgress.start();
NProgress.set(0.2);
//Increment 
var interval = setInterval(function() { NProgress.inc(); }, 1000);
$(document).ready(function(){
    NProgress.done();
    clearInterval(interval);
});


(function($){
	$.fn.preview = function(){
		var xOffset = 10;
		var yOffset = 20;
		var w = $(window).width();
		$(this).each(function(){
			$(this).hover(function(e){
				if($(this).html().toLowerCase().indexOf('ip')>-1){
					$("body").append("<div id='preview'><div><img src='https://images0.cnblogs.com/blog/82235/201401/201639088288.png' /></div></div>");
				}
				$("#preview").css({
					position:"absolute",					
					top:(e.pageY - yOffset) + "px",
					zIndex:9898
				});				
				if(e.pageX < w/2){
					$("#preview").css({
						left: e.pageX + xOffset + "px",
						right: "auto"
					}).fadeIn("fast");
				}else{
					$("#preview").css("right",(w - e.pageX + yOffset) + "px").css("left", "auto").fadeIn("fast");	
				}
			},function(){
				$("#preview").remove();
			}).mousemove(function(e){
				$("#preview").css("top",(e.pageY - xOffset) + "px")
				if(e.pageX < w/2){
					$("#preview").css("left",(e.pageX + yOffset) + "px").css("right","auto");
				}else{
					$("#preview").css("right",(w - e.pageX + yOffset) + "px").css("left","auto");
				}
			});						  
		});
	};
})(jQuery);
//$(function(){
	//$("a").preview();	  
//});
</script>

