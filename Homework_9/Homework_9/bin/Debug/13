function trim(str){
	return str.replace(/(^\s*)|(\s*$)/g, "");
}
function setStyle(a,b){
	var c=document.styleSheets[0];
	if(c.addRule){
	c.addRule(a,b);
	}else if(c.insertRule){
	c.insertRule(a+" { "+b+" }",c.cssRules.length);
	}}
function haoValidate(source){
	wval = trim(f.word.value);
	if (f.sc.value=="post")
